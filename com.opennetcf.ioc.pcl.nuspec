  <files>
    <!-- Portables -->
    <file src=".\bin\PCL\Release\OpenNETCF.IoC.dll" target="lib\portable-net45+netcore45+wpa81+wp8" />

    <!-- Android -->
    <file src=".\bin\PCL\Release\OpenNETCF.IoC.dll" target="lib\monoandroid+monotouch" />

    <!-- iOS -->
    <file src=".\bin\PCL\Release\OpenNETCF.IoC.dll" target="lib\Xamarin.iOS10" />
  </files>
